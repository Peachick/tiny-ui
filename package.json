{
  "name": "tiny-ui",
  "version": "0.0.93",
  "description": "A friendly ui component set for react",
  "license": "MIT",
  "keywords": [
    "tiny",
    "tiny-ui",
    "design",
    "react",
    "react-component",
    "component",
    "components",
    "ui",
    "frontend",
    "react hook component",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wangdicoder/tiny-ui.git"
  },
  "author": "Di Wang<wangdicoder@gmail.com>",
  "homepage": "https://tiny-ui.dev/",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "npm run build-ts && npm run dist",
    "commit": "git add . && ./node_modules/cz-customizable/standalone.js",
    "build-ts": "rimraf lib && tsc -p tsconfig.json",
    "dist-style": "gulp --gulpfile scripts/gulpfile.js && node scripts/generate-style.js",
    "dist-dev": "cross-env NODE_ENV=development webpack --config webpack.config.js --progress --colors",
    "dist-prod": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress --colors",
    "dist": "rimraf -rf dist && npm run dist-dev && npm run dist-prod && npm run dist-style",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config site/config/webpack.dev.js",
    "eslint": "eslint components --ext .ts,.tsx",
    "release": "standard-version",
    "site:build": "cross-env NODE_ENV=production webpack --config site/config/webpack.prod.js",
    "site:deploy": "cross-env NODE_ENV=production now",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.4.4",
    "classnames": "^2.2.6",
    "raf": "^3.4.1",
    "react-transition-group": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@commitlint/cli": "^11.0.0",
    "@mdx-js/loader": "^1.6.16",
    "@mdx-js/react": "^1.6.16",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/react-hooks": "^3.4.1",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.10",
    "@types/node": "^12.12.47",
    "@types/raf": "^3.4.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-transition-group": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "autoprefixer": "^9.8.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitlint-config-cz": "^0.13.2",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "cz-customizable": "^6.2.0",
    "eslint": "^7.4.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-jest-dom": "^3.0.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "file-loader": "^6.1.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "mini-css-extract-plugin": "^0.11.2",
    "node-sass": "^4.14.1",
    "postcss-loader": "^4.0.1",
    "prettier": "2.1.1",
    "prism-react-renderer": "^1.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-live": "^2.2.2",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.2",
    "standard-version": "^9.0.0",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.2.0",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "Firefox ESR",
    "IE >= 11",
    "Edge >= 14"
  ]
}
